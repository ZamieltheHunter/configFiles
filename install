!#/bin/sh/

mkdir -p ~/.oh-my-zsh/custom
ln -s ./custom/aliases.zsh ~/.oh-my-zsh/custom/
ln -s ./custom/exports.zsh ~/.oh-my-zsh/custom/
ln -s ./custom/shellproxy ~/.oh-my-zsh/custom/
ln -s zshrc ~/.zshrc

mkdir -p ~/.config/openbox/
ln -s ./openbox/rc.xml ~/.config/openbox/
ln -s .openbox/autostart ~/.config/openbox/

ln -s vim ~/.vim
ln -s vimrc ~/.vimrc

ln -s conkyrc ~/.conkyrc
